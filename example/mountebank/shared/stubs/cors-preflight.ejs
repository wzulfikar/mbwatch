{
  "predicates": [
    {
      "equals": {
        "method": "OPTIONS"
      }
    }
  ],
  "responses": [
    {
      "comment": "cors preflight response",
      "is": {
        "statusCode": "200",
        "headers": {
          <% include cors-headers %>
        }
      },
      "_behaviors": {
        "decorate": "<%- stringify(filename, 'shared/behaviors/cors-match-allow-origin.js') %>"
      }
    }
  ]
}
